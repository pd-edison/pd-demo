<body>
    <!-- Page Header -->
    <header class="masthead">
        <div class="container">
            <div class="jumbotron jumbotron-fluid">
                <h1>Ponddy Voice Conversion Demo Page</h1>
            </div>
        </div>
    </header>

    <!-- Main Content -->
    <div class="container">
        <table class="table table-striped">
            <thead>
                <tr>
                {% for title in titles%}
                    <th>{{title}}</th>
                {% endfor %}
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td colspan="{{ len(transcriptions) }}">GOP</td>
                </tr>
                <tr>
                    {% for gop in gops%}
                    <td>{{gop}}</td>
                    {% endfor %}
                </tr>
                {% for transcription, wavs in zip(transcriptions, wavss)%}
                <tr>
                    <td colspan="{{ len(transcriptions) }}">{{transcription}}</td>
                </tr>
                <tr>
                    {% for title, wav in zip(titles, wavs) %}
                    <td>
                        {{ title }}
                        <audio controls preload="none" style="width: 200px">
                            <source src="{{ wav }}">
                        </audio>
                    </td>
                    {% endfor %}
                </tr>
                {% endfor %}
            </tbody>
        </table>

        <!-- Footer -->
        <foo ter>
